package javastudy260113;

public class User {
	
	private String name;
	private String password;
	private String id;
	private String tal;
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getPassword() {
		return password;
	}
	public void setPassword(String password) {
		this.password = password;
	}
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getTal() {
		return tal;
	}
	public void setTal(String tal) {
		this.tal = tal;
	}
	
	@Override
	public String toString() {
		return "User [사용자: " + name + ", 비밀번호: " + password + ", ID: " + id + ", 연락처: " + tal + "]";
	}

1. 로그인의 방법 3번에 따라 추가 필요 
	public User() { 
	}

	public User(String name, String password, String id, String tal) {
		super();
		this.name = name;
		this.password = password;
		this.id = id;
		this.tal = tal;
	}
	
	
	
	

}
