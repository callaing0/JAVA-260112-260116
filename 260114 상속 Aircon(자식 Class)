package products;

import common.Appliance;

//에어컨 extends+(부모 클래스 작성) : 상속처리
public class Aircon extends Appliance {
	
	public Aircon() {
	}
	
	// super : 부모클래스의 생성자 매서드를 실행
	public Aircon(String name, int pwerCon) {
		super(name, powerCon);
	}
	
	@Override
	public void power() {
		onoff = !onoff;
		if(onoff) {
            System.out.println("냉장을 시작합니다.");
        } else {
            System.out.println("자동 건조를 시작합니다. 전원을 종료합니다.");
        }
	}
	
	
	

}
